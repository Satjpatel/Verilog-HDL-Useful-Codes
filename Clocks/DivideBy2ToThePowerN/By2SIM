    %jmp/0xz  T_2.0, 5;
    %load/vec4 v0x55b4422e76a0_0;
    %addi 1, 0, 5;
    %store/vec4 v0x55b4422e76a0_0, 0, 5;
    %jmp T_2.1;
T_2.0 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x55b4422e76a0_0, 0, 5;
T_2.1 ;
